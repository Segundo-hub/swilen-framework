{
    "name": "swilen/framework",
    "description": "Fast Routing inspired in Laravel and Expressjs",
    "license": "MIT",
    "version": "0.7.0-alpha",
    "keywords": [
        "routing",
        "framework",
        "micro-framework",
        "swilen"
    ],
    "homepage": "https://segundo-hub.github.io/swilen/",
    "authors": [
        {
            "name": "Alex Segundo",
            "email": "asegundo442@gmail.com",
            "homepage": "https://alexsegundo.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Swilen\\": "src"
        },
        "files": [
            "src/Arthropod/helpers.php",
            "src/Shared/functions.php"
        ]
    },
    "require": {
        "php": ">=7.3",
        "ext-mbstring": "*",
        "ext-json": "*",
        "psr/container": "^2.0",
        "psr/log": "^1.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "mockery/mockery": "^1.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "scripts": {
        "test-coverage": "phpdbg -qrr ./vendor/bin/pest --coverage --min=90"
    }
}
