{
    "name": "swilen/framework",
    "description": "Fast Routing inspired in Laravel and Expressjs",
    "license": "MIT",
    "version": "0.8.0",
    "keywords": ["routing", "framework", "micro-framework", "swilen"],
    "homepage": "https://segundo-hub.github.io/swilen/",
    "authors": [
        {
            "name": "Alex Segundo",
            "email": "asegundo442@gmail.com",
            "homepage": "https://alexsegundo.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Swilen\\": "src"
        },
        "files": [
            "src/Arthropod/helpers.php"
        ]
    },
    "require": {
        "php": ">=7.3",
        "psr/container": "^2.0",
        "psr/log": "^1.1",
        "phpmailer/phpmailer": "^6.6",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "pestphp/pest": "^1.21"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
